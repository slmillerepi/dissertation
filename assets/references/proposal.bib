  author = {Thompson, Mark G. and Pierse, Nevil and Sue Huang, Q. and Prasad, Namrata and Duque, Jazmin and Claire Newbern, E. and Baker, Michael G. and Turner, Nikki and McArthur, Colin and {SHIVERS investigation team}},
  date = {2018-09-18},
