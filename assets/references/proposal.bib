  keywords = {Antibodies,Influenza virus,Viral epidemiology},
  file = {/Users/savannahhammerton/Zotero/storage/L7CISDXG/Edler et al. - 2024 - Immune imprinting in early life shapes cross-react.pdf}
}

@article{escudero-perez23,
  title = {Immune Correlates of Protection for {{SARS-CoV-2}}, {{Ebola}} and {{Nipah}} Virus Infection},
  author = {Escudero-Pérez, Beatriz and Lawrence, Philip and Castillo-Olivares, Javier},
