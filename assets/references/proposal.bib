  author = {Ip, Dennis K. M. and Lau, Lincoln L. H. and Leung, Nancy H. L. and Fang, Vicky J. and Chan, Kwok-Hung and Chu, Daniel K. W. and Leung, Gabriel M. and Peiris, J. S. Malik and Uyeki, Timothy M. and Cowling, Benjamin J.},
